{
  "algorithm": "cpc",
  "data_dir": "kaggle_retina_2019/images/resized_224",
  "train_data_generator_args": {"augment": true, "shuffle": true},
  "val_data_generator_args": {"augment": false},
  "dataset_name": "kaggle_retina",

  "data_is_3D": false,
  "data_dim": 224,
